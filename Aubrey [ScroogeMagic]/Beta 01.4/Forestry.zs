for i, log in logs {
    var plank = planks[i];

   recipes.addShapeless(plank*2,[log]);

   recipes.addShapeless(plank*4,[saws.anyDamage(),log]);
   recipes.addShapeless(plank*4,[sawi.anyDamage(),log]);
   recipes.addShapeless(plank*4,[sawd.anyDamage(),log]);
}

var stairs=[fs,fs1,fs2,fs3,fs4,fs5,fs6,fs7,fs8,fs9,fs10,fs11,fs12,fs13,fs14,fs15,fs16,fs17,fs18,fs19,fs20,fs21,fs22,fs23,fs24,fs25,fs26,fs27,fs28] as IItemStack[];

for i, planks in planks {
    var stair = stairs[i];

    recipes.addShaped(stair*4,
    [[planks,null,saws.anyDamage()],
    [planks,planks,null],
    [planks,planks,planks]]);

    recipes.addShaped(stair*4,
    [[planks,null,sawi.anyDamage()],
    [planks,planks,null],
    [planks,planks,planks]]);

    recipes.addShaped(stair*4,
    [[planks,null,sawd.anyDamage()],
    [planks,planks,null],
    [planks,planks,planks]]);
}

